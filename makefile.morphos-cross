#export PATH=$PATH:/gg/bin

B = buildmos
BID = $(B)/idlib
BB  = $(B)/base
B3  = $(B)/d3xp
BR  = $(B)/doom

CXX = ppc-morphos-g++ -mcpu=750 -mtune=7450
CC = ppc-morphos-gcc -mcpu=750 -mtune=7450
STRIP = ppc-morphos-strip
AR = ppc-morphos-ar
RANLIB = ppc-morphos-ranlib

#PATH_SOURCE = /home/cowcat/Escritorio/Cross/dhewm3
PATH_SOURCE = ./

CXX_FLAGS = -Wall -DNDEBUG -O2 -ffast-math -noixemul -fvisibility=hidden -fomit-frame-pointer -fno-strict-aliasing -Wno-sign-compare -Wno-switch -Wno-format-security -fpermissive -Wno-class-memaccess
CC_FLAGS = -Wall -DNDEBUG -O2 -ffast-math -noixemul -fvisibility=hidden -fomit-frame-pointer -fno-strict-aliasing -Wno-sign-compare -Wno-switch -Wno-format-security

CXX_INCLUDES = -I$(PATH_SOURCE) -I$(PATH_SOURCE)neo -I/gg/usr/local/include/SDL2 -I/gg/usr/local/include
CXX_INCLUDES_BASE = -I$(PATH_SOURCE) -I$(PATH_SOURCE)neo -I$(PATH_SOURCE)neo/game -I/gg/usr/local/include/SDL2
CXX_INCLUDES_D3XP = -I$(PATH_SOURCE) -I$(PATH_SOURCE)neo -I$(PATH_SOURCE)neo/d3xp -I/gg/usr/local/include/SDL2
CXX_DEFINES = -D__DOOM_DLL__
CXX_DEFINES_BASE = -DGAME_DLL -Dbase_EXPORTS
CXX_DEFINES_D3XP = -DGAME_DLL -D_D3XP -DCTF -Dd3xp_EXPORTS
CXX_DEFINES_ALTIVEC = -D__VECTOR__

DO_CC = $(CC) $(CC_FLAGS) $(C_DEFINES) $(CXX_INCLUDES) -o $@ -c $<
DO_CXX = $(CXX) $(CXX_FLAGS) $(CXX_DEFINES) $(CXX_INCLUDES) -o $@ -c $<
DO_CXX_ALTIVEC = $(CXX) $(CXX_FLAGS) $(CXX_DEFINES) $(CXX_DEFINES_ALTIVEC) $(CXX_INCLUDES) -fvec -maltivec -faltivec -mabi=altivec -o $@ -c $<
DO_CXX_B = $(CXX) $(CXX_FLAGS) $(CXX_DEFINES_BASE) $(CXX_INCLUDES_BASE) -o $@ -c $<
DO_CXX_XP = $(CXX) $(CXX_FLAGS) $(CXX_DEFINES_D3XP) $(CXX_INCLUDES_D3XP) -o $@ -c $<

