IDLIB = $(B)/idlib.a
BASE = $(B)/base.so
D3XP = $(B)/d3xp.so
DOOM = $(B)/doom3

LIBS = $(IDLIB) -noixemul -L/gg/usr/local/lib -lSDL2 -lopenal -lvorbisfile -lvorbis -logg -lpthread -ldll

all: $(IDLIB) $(BASE) $(D3XP) $(DOOM)  

$(IDLIB): $(OBJIDLIB)
	$(AR) r $(IDLIB) $(OBJIDLIB)
	$(RANLIB) $@

$(BASE): $(OBJBASE)
	$(CXX) -o $@ $(OBJBASE) $(IDLIB) -noixemul -ldll
	$(STRIP) $@

$(D3XP): $(OBJD3XP)
	$(CXX) -o $@ $(OBJD3XP) $(IDLIB) -noixemul -ldll
	$(STRIP) $@
	
$(DOOM): $(OBJDOOM)
	$(CXX) -o $@ $(OBJDOOM) $(LIBS)
	$(STRIP) $@

